# Solana Trading Bot Environment Example
# Network
SOLANA_RPC_ENDPOINT=https://api.mainnet-beta.solana.com
SOLANA_WS_ENDPOINT=wss://api.mainnet-beta.solana.com
SOLANA_COMMITMENT=confirmed

# API Keys
HELIUS_API_KEY=your_helius_api_key
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_telegram_chat_id
PYTH_API_KEY=your_pyth_api_key
SWITCHBOARD_API_KEY=your_switchboard_api_key

# Wallet
WALLET_PUBLIC_KEY=your_wallet_public_key
WALLET_PRIVATE_KEY=your_wallet_private_key_base58

# Trading Parameters
MAX_POSITION_SIZE=1.0
MIN_LIQUIDITY_USD=10000
MAX_SLIPPAGE_BPS=150
MIN_HOLDERS=100

# Risk Management
MAX_DRAWDOWN=0.1
STOP_LOSS_PERCENTAGE=0.05
TAKE_PROFIT_PERCENTAGE=0.1
DAILY_LOSS_LIMIT=0.05

# Monitoring
TELEGRAM_NOTIFY_TRADES=true
LOG_LEVEL=info
PROMETHEUS_ENABLED=true
METRICS_PORT=9090

# Model Settings
MODEL_PATH=./models/solana_bot_model.onnx
CONFIDENCE_THRESHOLD=0.67

# Database
DB_PATH=./data/trades.db

# Scam Wallets (comma-separated)
SCAM_WALLETS=